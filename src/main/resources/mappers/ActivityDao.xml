<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.ActivityDao">
    <resultMap id="ActivityMap" type="com.example.demo.Model.Activity">
        <id column="actid" jdbcType="BIGINT" property="actid"/>
        <result column="aid" jdbcType="BIGINT" property="aid"/>
        <result column="actname" jdbcType="VARCHAR" property="actname"/>
        <result column="time" jdbcType="TIME" property="time"/>
    </resultMap>
    <delete id="deleteActivity" parameterType="long">
    DELETE FROM user where actid = #{actid}
</delete>
</mapper>
