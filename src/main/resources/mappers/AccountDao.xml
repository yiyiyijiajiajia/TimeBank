<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.AccountDao">
    <resultMap id="AccountMap" type="com.example.demo.Model.Account">
        <id column="aid" jdbcType="BIGINT" property="aid"/>
        <result column="uid" jdbcType="BIGINT" property="uid"/>
        <result column="aname" jdbcType="VARCHAR" property="aname"/>
        <result column="atime" jdbcType="BIGINT" property="atime"/>
        <result column="retime" jdbcType="BIGINT" property="retime"/>
    </resultMap>
    <delete id="deleteByAid" parameterType="long">
        DELETE FROM account where aid = #{aid}
    </delete>
    <insert id="insertAccount" parameterType="Account">
        INSERT INTO account(aid, uid, aname, atime, retime)Values(#{aid}, #{uid}, #{aname}, #{atime}, #{retime})
    </insert>

</mapper>