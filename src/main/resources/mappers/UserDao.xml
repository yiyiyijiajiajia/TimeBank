<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.UserDao">
<resultMap id="UserMap" type="com.example.demo.Model.User">
    <id column="uid" jdbcType="BIGINT" property="uid"/>
    <result column="life" jdbcType="INTEGER" property="life"/>
    <result column="birthday" jdbcType="VARCHAR" property="birthday"/>
    <result column="relife" jdbcType="INTEGER" property="relife"/>
    <result column="spentlife" jdbcType="INTEGER" property="spentlife"/>
    <result column="phone" jdbcType="VARCHAR" property="phone"/>
    <result column="password" jdbcType="VARCHAR" property="password"/>
</resultMap>
<delete id="deleteUser" parameterType="long">
    DELETE FROM user where uid = #{uid}
</delete>
</mapper>